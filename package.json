{
	"name": "rststore",
	"version": "1.0.0",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"start": "node server/main.js",
		"server": "nodemon server/main.js",
		"client": "npm run dev --prefix client",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"data:import": "node server/seeder",
		"data:destroy": "node server/seeder -d"
	},
	"imports": {
		"#config/*": "./server/config/*",
		"#data/*": "./server/data/*",
		"#models/*": "./server/models/*",
		"#routes/*": "./server/routes/*",
		"#middlewares/*": "./server/middlewares/*",
		"#controllers/*": "./server/controllers/*",
		"#utils/*": "./server/utils/*"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@gitlab.com/rstforumnet/rststore-daily-evening-december-2025.git"
	},
	"author": "Rahul Sharma",
	"license": "UNLICENCED",
	"bugs": {
		"url": "https://gitlab.com/rstforumnet/rststore-daily-evening-december-2025/issues"
	},
	"homepage": "https://gitlab.com/rstforumnet/rststore-daily-evening-december-2025#readme",
	"description": "",
	"dependencies": {
		"bcryptjs": "^3.0.3",
		"colors": "^1.4.0",
		"cookie-parser": "^1.4.7",
		"dotenv": "^17.2.3",
		"express": "^5.2.1",
		"jsonwebtoken": "^9.0.3",
		"mongoose": "^9.1.1",
		"morgan": "^1.10.1",
		"multer": "^2.0.2"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.28.5",
		"concurrently": "^9.2.1",
		"nodemon": "^3.1.11"
	}
}
